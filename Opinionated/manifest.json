{
  "name": "Opinionated",
  "version": "1.2.1",
  "manifest_version": 2,
  "description": "Warns you about untrustworthy sites",
  "homepage_url": "https://github.com/akhilgupta1093/opinionated",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://*.cnn.com/*",
    "https://*.breitbart.com/*",
    "https://*.bbc.com/*",
    "https://*.msnbc.com/*",
    "https://*.nytimes.com/*",
    "https://*.wsj.com/*",
    "https://*.foxnews.com/*",
    "https://*.huffpost.com/*",
    "https://*.washingtonpost.com/*",
    "https://*.latimes.com/*",
    "https://*.reuters.com/*",
    "https://*.abcnews.go.com/*",
    "https://*.usatoday.com/*",
    "https://*.bloomberg.com/*",
    "https://*.nbcnews.com/*",
    "https://*.dailymail.co.uk/*",
    "https://*.theguardian.com/*",
    "https://*.indiatimes.com/*",
    "https://*.news.com/*",
    "https://*.politico.com/*",
    "https://*.nypost.com/*",
    "https://*.nbcnews.com/*",
    "https://*.cbsnews.com/*",
    "https://*.abcnews.com/*",
    "https://*.denverpost.com/*",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.cnn.com/*",
        "https://*.breitbart.com/*",
        "https://*.bbc.com/*",
        "https://*.msnbc.com/*",
        "https://*.nytimes.com/*",
        "https://*.wsj.com/*",
        "https://*.foxnews.com/*",
        "https://*.huffpost.com/*",
        "https://*.washingtonpost.com/*",
        "https://*.latimes.com/*",
        "https://*.reuters.com/*",
        "https://*.abcnews.go.com/*",
        "https://*.usatoday.com/*",
        "https://*.bloomberg.com/*",
        "https://*.nbcnews.com/*",
        "https://*.dailymail.co.uk/*",
        "https://*.theguardian.com/*",
        "https://*.indiatimes.com/*",
        "https://*.news.com/*",
        "https://*.politico.com/*",
        "https://*.nypost.com/*",
        "https://*.nbcnews.com/*",
        "https://*.cbsnews.com/*",
        "https://*.abcnews.com/*",
        "https://*.denverpost.com/*"
      ],
      "js": ["inject.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Opinionated"
  }
}
